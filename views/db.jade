extends layout

block content
  div(id="main")
    if loggedIn
      h2 The DB&hellip;
      h3 Most Recent Bookmarks
      p
        a(href="../api/recent") JSON
      table(class="table-db")
        tr
          th Link
          th Name
          th Description
          th Category
          th Date
        each row in rows
          tr
            each value in row
              td= value

